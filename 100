for (Map.Entry<String, String> e : kvBlock) {            // kvBlock es List<Map.Entry<...>>
    String keyWithSuffix = e.getKey();
    String value        = e.getValue();
    String baseKey      = keyWithSuffix.substring(0, keyWithSuffix.length() - sufijo.length());

    // NO saltar numeroDeclaracion
    if (shouldSkip(baseKey)) {
        skips.add(baseKey);
        continue;
    }

    // --- normalización especial para numeroDeclaracion ---
    String valueToSearch = value;
    if (baseKey.endsWith("_numeroDeclaracion")) {
        // quita todo lo que no sea dígito y elimina ceros a la izquierda
        String digits = value.replaceAll("\\D", "");
        digits = digits.replaceFirst("^0+(?!$)", "");   // “00054825” -> “54825”
        valueToSearch = digits;
    }

    boolean match = containsJsonValue(rowJson, valueToSearch);  // tu helper flexible

    String printable = baseKey + " = " + value;
    if (match)  oks.add(printable);
    else        fails.add(printable);
}
