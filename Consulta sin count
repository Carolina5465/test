SELECT 
  b.FP_NUMERODECLARACION,
  TO_CHAR(b.FP_FECHA, 'DD-MON-RR', 'NLS_DATE_LANGUAGE=AMERICAN') AS FP_FECHA,
  b.NOMBREARCHIVO
FROM BAC_DETFORMULARIOSPROCESADOS_XML a
JOIN BAC_FORMULARIOSPROCESADOS_XML b
  ON a.FP_NOMBREARCHIVO = b.NOMBREARCHIVO
WHERE a.FP_NOMBREARCHIVO = 'DC-N_860051135_251007221423.xml'
ORDER BY b.FP_NUMERODECLARACION, b.FP_FECHA;
